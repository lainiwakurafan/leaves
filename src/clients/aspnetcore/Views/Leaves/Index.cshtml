@model Leaves.MvcClient.DataContracts.GetLeavesContract

<table>
    <thead>
        <tr>
            <th>id</th>
            <th>start</th>
            <th>end</th>
            <th>status</th>
            <th>link</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items) {
            <tr>
                <td>
                    @Html.DisplayFor(x => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(x => item.Start)
                </td>
                <td>
                    @Html.DisplayFor(x => item.End)
                </td>
                <td>
                    @Html.DisplayFor(x => item.Status)
                </td>
                <td>
                    @if (String.IsNullOrEmpty(item.GoogleCalendarLink)) {
                        <span>N/A</span>
                    } else {
                        <a href="@item.GoogleCalendarLink">link</a>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
